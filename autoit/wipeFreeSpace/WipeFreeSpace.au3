; Includes und Basiskonfig
; ======================================================================= ;
#include <GUIConstants.au3>
#include <Array.au3>
#NoTrayIcon
HotKeySet("+!d", "Terminate")  ;Shift-Alt-d
Opt("GUICloseOnESC", 0)
Opt("MustDeclareVars", 1)
Opt("TrayIconHide", 1);

; ======================================================================= ;
; Create GUI
; ======================================================================= ;
GUICreate("Harddrive Wipe", 350, 150, -1, -1)  ; will create a dialog box that when displayed is centered

Dim $lbl_01 = GUICtrlCreateLabel("Select drive for FreeSpaceWipe: ", 10, 13, 175, 15);
Dim $var = DriveGetDrive ("ALL");
If NOT @error Then
	For $i = 1 to $var[0]
		If $i = 1 Then
			Dim $cbo_01 = GUICtrlCreateCombo (StringUpper($var[$i]) & " [" & DriveGetLabel($var[$i]) & "]", 180, 10, 160, -1, $CBS_DROPDOWNLIST) ; create first item
		Else
			GUICtrlSetData($cbo_01, StringUpper($var[$i]) & " [" & DriveGetLabel($var[$i]) & "]", $var[1])
		EndIf
	Next
EndIf

Dim $lbl_02 = GUICtrlCreateLabel("Free Space on drive """ & StringUpper($var[1]) & """ : " & Round(DriveSpaceFree($var[1])) & "MB", 10, 35, 225, 15)

Dim $btn_01 = GUICtrlCreateButton("&Start Wipe", 10, 70, -1, -1);
Dim $btn_02 = GUICtrlCreateButton("S&top Wipe", 90, 70, -1, -1);
GUISetState ()




; ======================================================================= ;
; Run the GUI until the dialog is closed
; ======================================================================= ;
Dim $msg;
While 1
    $msg = GUIGetMsg()
   
    If $msg = $GUI_EVENT_CLOSE Then ExitLoop
	If $msg = $cbo_01 Then
		ChangeLabelFreeSpace(GUICtrlRead($cbo_01))
	EndIf
	If $msg = $btn_01 Then
		StartWipe(GUICtrlRead($cbo_01))
	EndIf
Wend

Func ChangeLabelFreeSpace($box)
	Dim $drive = StringSplit($box, " ");
	GUICtrlSetData($lbl_02, "Free Space on drive """ & $drive[1] & """ : " & Round(DriveSpaceFree($drive[1])) & "MB");
EndFunc

Func StartWipe($box)
	Dim $drive = StringSplit($box, " ");
	If FileExists($drive[1]) Then
		Dim $iMsgBoxAnswer
		Dim $i = 0;
		Dim $WipeFile;
		Dim $WipeRtn;
		Dim $CreatedFiles[1];
		Dim $MaxFileSize;
		_ArrayDelete($CreatedFiles, 0)
		$iMsgBoxAnswer = MsgBox(276,"Sind Sie sicher?","Wollen Sie wirklich die angegebe Partition wirklich bis zum Ende beschreiben?" & @CRLF & "Sollte es sich um die Systempartition handeln, wird Ihr System nicht neu starten")
		If $iMsgBoxAnswer = 6 Then;Yes
			GUICtrlSetState($btn_01, $GUI_DISABLE);
			GUICtrlSetState($cbo_01, $GUI_DISABLE);
			Dim $FileSystem = DriveGetFileSystem($drive[1])
			
			Switch $FileSystem
				Case "FAT"
					$MaxFileSize = 2000000000
				Case "FAT32"
					$MaxFileSize = 4000000000
				Case "NTFS"
					$MaxFileSize = Ceiling(DriveSpaceFree($drive[1]) * 1024 * 1024)
				Case Else
					$MaxFileSize = 1000000000
			EndSwitch

			While DriveSpaceFree($drive[1]) > 0.01
				$WipeFile = "winpage" & $i & ".sys";
				$WipeRtn  = WriteWipeFile($WipeFile, $drive[1], $MaxFileSize)
				If $WipeRtn = 0 Then
					_ArrayAdd($CreatedFiles, $drive[1] & "\" & $WipeFile);
					ExitLoop
				Else
					_ArrayAdd($CreatedFiles, $drive[1] & "\" & $WipeFile);
				EndIf
				$i = $i + 1;
			WEnd

			$iMsgBoxAnswer = MsgBox(36,"Wipe finished","Wiping process finished." & @CRLF & "Do you want to delete the created temp files?")
			If $iMsgBoxAnswer = 6 Then;Yes
				FOR $file IN $CreatedFiles
					If $file <> "" Then
						If FileExists($file) Then
							FileDelete($file);
						EndIf
					EndIf
				NEXT
			EndIf
			GUICtrlSetState($btn_01, $GUI_ENABLE);
			GUICtrlSetState($cbo_01, $GUI_ENABLE);
		EndIf
	EndIf
EndFunc

